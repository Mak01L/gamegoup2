{
  "version": 2,
  "buildCommand": "chmod +x ./vercel-build.sh && ./vercel-build.sh",
  "outputDirectory": "dist",
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)\\.js", "dest": "/$1.js" },
    { "src": "/(.*)\\.css", "dest": "/$1.css" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}